        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
      <div class="carousel-inner">
        <div class="item active">
          <%= image_tag @home.filepicker_url, :class => "img-responsive" %>
        </div>
        <div class="item">
          <%= image_tag @home.filepicker_url, :class => "img-responsive" %>
        </div>
        <div class="item">
          <%= image_tag @home.filepicker_url, :class => "img-responsive" %>
        </div>
      </div>
    </div><!-- /.carousel -->
  </div>
</div>
